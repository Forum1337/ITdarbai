#include <iostream>
#include <fstream>

const char D[] = "d.txt";
const int CMAX = 100;
using namespace std;

  int A[CMAX];
  int n;
  
int
main ()
{


  return 0;
}

void
skaityti ()
{
  ifstream d (D);

  d >> n;
  for (int i = 0; i < n; i++)
    {
      d >> A[i];
      cout << i << " " << A[i] << endl;
    }
  d.close ();



}
